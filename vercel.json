{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "build": {
    "env": {
      "SKIP_BUILD": "true"
    }
  },
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
} 